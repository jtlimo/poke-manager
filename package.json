  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.10.0",
